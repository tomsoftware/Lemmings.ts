{
  "version": "0.2.0",
  "configurations": [
          {
          "name": "Launch file",
          "type": "chrome",
          "request": "launch",
          "url": "http://localhost:8888/lemmings.html",
          "webRoot": "${workspaceRoot}\\run\\"
      },
      {
          "name": "Launch localhost",
          "type": "chrome",
          "request": "launch",
